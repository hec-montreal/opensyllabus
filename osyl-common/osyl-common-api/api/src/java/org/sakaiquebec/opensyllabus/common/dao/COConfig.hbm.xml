<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.sakaiquebec.opensyllabus.shared.model.COConfigSerialized" table="osyl_co_config">
        <id name="configId" type="string">
            <column name="CONFIG_ID" length="99" />
            <generator class="assigned" />
        </id>
       
        <property name="configRef" type="text">
            <column name="CONFIGREF" />
        </property>
        
        <property name="description" type="string">
            <column name="DESCRIPTION" />
        </property>
        
 	</class>


</hibernate-mapping>