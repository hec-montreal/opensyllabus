<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<!-- list the backing beans here -->
	<bean id="osylPortalMainBean"
			class="org.sakaiquebec.opensyllabus.portal.server.OsylPortalBackingBean"
			init-method="init">
		<property name="osylService"
			ref="org.sakaiquebec.opensyllabus.api.OsylService" />
		<property name="osylSiteService"
			ref="org.sakaiquebec.opensyllabus.common.api.OsylSiteService" />			
		<property name="osylPublishService"
			ref="org.sakaiquebec.opensyllabus.common.api.OsylPublishService" />
		<property name="osylSecurityService" 
			ref="org.sakaiquebec.opensyllabus.common.api.OsylSecurityService" />
		<property name="osylConfigService" 
			ref="org.sakaiquebec.opensyllabus.common.api.OsylConfigService" />
	</bean>

</beans>